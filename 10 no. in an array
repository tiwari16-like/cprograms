#include<stdio.h>
 void main()
 {
     int a[10],i;
     printf("enter 10 values:\n");
     for(i=0;i<10;i++)
     {
         printf("enter a value at %d position",i+1);
         scanf("%d",&a[i]);
     }
     printf("\n you have entered:\n");
     for(i=0;i<10;i++)
     {
         print("%d\n",a[i]);
     }
 }
